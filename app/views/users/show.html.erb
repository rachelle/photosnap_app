<h2 class="collect">this is <%= @user.name %> photos</h4>

<table class="table table-hover">

<% @user_photos.each do |photo| %>
  Title: <%= photo.title %> 
  <br>
  Url:<%= photo.url %> 
  <br>
  <%= image_tag photo.image %>
  <% if current_user === @user %>
    <%= link_to 'edit photo', edit_photo_path(photo) %> 
</table>


  <% end %>
<% end %>
 

