
<div class= "feed">
  <h2>Your <%= @user.name %>profile</h2>
</div>

<div class= "text-center">
<div class= "container">

<table class="table table-hover">

<% @user_photos.each do |photo| %>
  <h3>Title: <%= photo.title %></h3>
 
  <h3>Url:<%= photo.url %></h3>
  <%= image_tag photo.image %>

  <% if current_user === @user %>
   <h3> <%= link_to 'edit photo', edit_photo_path(photo) %></h3>
   <h3>
    <%= button_to "delete", @photo, :method => :delete, class: 'delete-btn' %>
</table>



  <% end %>
<% end %>
</div>
</div>
 

