<div class="text-center">
  <div class="feed">
    <h1>Photo Feed</h1>
  </div>
  <br>
  <div class="container">
  <% @photos.each do |photo| %>
     <div class="col-md-4">
      <h3>

      Username: <%= photo.user.name %>
      </h3>
       <br>
      <%= link_to (image_tag photo.image(:medium)), photo_path(photo.id), class: 'feed-pictures' %>
      <br>
      <h3>
      Caption:
      <%= photo.caption %>
    </h3>
  </div>
     <% end %>
  </div>
</div>





     
