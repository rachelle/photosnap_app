<div class="text-center">
  <div class="feed">
    <div class="header">
      <h1>Photo Feed</h1>
    </div>
  </div>
  <br>
  <div class="container">
  <% @photos.each do |photo| %>
    <div class="col-md-4">
      <br>
      <%= link_to (image_tag photo.image(:medium)), photo_path(photo.id), class: 'feed-pictures' %>
      <br>
      </div>
    <% end %>
  </div>
</div>





     
